<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="AlertDialogStyle" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:colorAccent">#FF73706E</item>
        <item name="android:textColor">#73706E</item>
        <item name="android:textColorPrimary">#FF73706E</item>
    </style>

    <style name="AlertDialogLogout" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:colorAccent">#ff5500</item>
        <item name="android:textColor">#ff5500</item>
        <item name="android:textColorPrimary">#ff5500</item>
    </style>


    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />



</resources>
